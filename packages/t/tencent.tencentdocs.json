{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "Tencent.TencentDocs",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://dldir1.qq.com/weiyun/tencentdocs/electron-update/release/latest-win32.yml",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response | ConvertFrom-Yaml",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "2.2.33",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "\"https://dldir1.qq.com/weiyun/tencentdocs/electron-update/release/$($Response.path)\"",
    "ReleaseDate": "(Get-Date -Date $Response.releaseDate).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
