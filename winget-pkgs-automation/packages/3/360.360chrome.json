{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "360.360Chrome",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://chrome.360.cn/m/ver.ini",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response.Split(':')[1]",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "13.5.2015.0",
  "ManifestFields": {
    "PackageVersion": "$Response",
    "InstallerUrls": "\"https://down.360safe.com/cse/360cse_$($Response).exe\""
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
