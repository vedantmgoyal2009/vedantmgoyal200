{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "JetBrains.CLion",
  "Update": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://data.services.jetbrains.com/products/releases?latest=true&type=release&code=CL",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.CL",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".*.exe$",
  "PreviousVersion": "",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "($Response.downloads.windows.link -replace \"https://download.jetbrains.com\",\"https://download-cdn.jetbrains.com\")",
    "ReleaseDate": "$Response.date",
    "AppsAndFeaturesEntries": [
      {
        "DisplayName": "'CLion ' + $Response.majorVersion",
        "DisplayVersion": "$Response.build",
        "ProductCode": "'CLion ' + $Response.majorVersion"
      }
    ],
    "Locales": [
      {
        "Name": "en-us",
        "ReleaseNotesUrl": "$Response.notesLink"
      }
    ]
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "SkipPRCheck": false,
  "SkipPackage": false
}
