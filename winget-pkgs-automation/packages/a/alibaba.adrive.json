{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "Alibaba.aDrive",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://www.aliyundrive.com/desktop/version/update.json",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = Invoke-RestMethod -Uri \"$($Response.url)/win32/ia32/latest.yml\" | ConvertFrom-Yaml",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "3.2.0",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "\"https://yunpan.aliyun.com/downloads/apps/desktop/aDrive-$($Response.version).exe\""
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
