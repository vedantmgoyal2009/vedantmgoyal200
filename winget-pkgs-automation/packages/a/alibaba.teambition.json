{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "Alibaba.Teambition",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://im.dingtalk.com/manifest/dtron/Teambition/win32/ia32/latest.yml",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response | ConvertFrom-Yaml",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "2.0.2",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "$Response.path",
    "ReleaseDate": "(Get-Date -Date $Response.releaseDate).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
