{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "Amazon.AWSCLI",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://api.github.com/repos/aws/aws-cli/tags?per_page=1",
    "Method": "Get",
    "Headers": {
      "Authorization": "$AuthToken",
      "Accept": "application/vnd.github.v3+json"
    },
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "2.6.1.0",
  "ManifestFields": {
    "PackageVersion": "\"$($Response.name).0\"",
    "InstallerUrls": "\"https://awscli.amazonaws.com/AWSCLIV2-$($Response.name).msi\""
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
