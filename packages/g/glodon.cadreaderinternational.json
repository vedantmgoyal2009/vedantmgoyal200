{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "Glodon.CADReaderInternational",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://cadreader.glodon.com/query/update/cadpc?clientVersion=3.4.3.12&cadpcClientBits=32",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response.data",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "3.5.0.13",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "$Response.url"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
