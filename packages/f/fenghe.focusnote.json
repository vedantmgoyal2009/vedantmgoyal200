{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "FengHe.FocusNote",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://focusnote.com.cn/admin/version/listNew",
    "Method": "Post",
    "Headers": {},
    "Body": "platform=0&prodNo=0",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response.data[0].downloadUrl",
  "VersionRegex": "[0-9.-]{2,}(?=\\.)",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "",
  "ManifestFields": {
    "PackageVersion": "([System.Uri]::UnescapeDataString($Response) | Select-String -Pattern $VersionRegex).Matches.Value",
    "InstallerUrls": "$Response"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
