{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "Element.Element",
  "Update": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://api.github.com/repos/vector-im/element-web/releases?per_page=1",
      "Method": "Get",
      "Headers": {
        "Authorization": "$AuthToken",
        "Accept": "application/vnd.github.v3+json"
      },
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$UpdateCondition = $Response.prerelease -eq $PreRelease -and $Response.id -gt $PreviousReleaseId",
  "VersionRegex": "(?<=v)[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "1.11.48",
  "ManifestFields": {
    "PackageVersion": "$Response.tag_name.TrimStart('v')",
    "InstallerUrls": "\"https://packages.element.io/desktop/install/win32/x64/Element%20Setup%20$($Response.tag_name.TrimStart('v')).exe\"",
    "ReleaseDate": "(Get-Date -Date $Response.published_at).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {
    "PreRelease": false,
    "PreviousReleaseId": 128344652
  },
  "PostUpgradeScript": "$Package.AdditionalInfo.PreviousReleaseId = $Response.id",
  "SkipPRCheck": false,
  "SkipPackage": false
}
